{
  "name": "genji",
  "version": "1.0.0",
  "description": "a vue state management framewok by vue3 reactivity api",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --fix --ext .js,.ts",
    "format": "prettier --write \"src/**/*.ts\" \"example/**/*.ts\" \"example/**/*.vue\"",
    "commit": "git-cz",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xieyezi/juefei.git"
  },
  "author": "xieyezi",
  "license": "MIT",
  "keywords": [
    "vue3",
    "vue3 reactivity",
    "vue3 hooks",
    "hooks",
    "redux",
    "vuex",
    "vue-composition api",
    "dva",
    "state",
    "manager",
    "management",
    "store"
  ],
  "bugs": {
    "url": "https://github.com/xieyezi/juefei/issues"
  },
  "homepage": "https://github.com/xieyezi/juefei#readme",
  "dependencies": {
    "@vue/reactivity": "^3.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@typescript-eslint/parser": "^4.23.0",
    "commitizen": "^4.0.3",
    "cz-customizable": "^6.2.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.3.6",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "cz-customizable": {
    "config": ".cz-config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  }
}
